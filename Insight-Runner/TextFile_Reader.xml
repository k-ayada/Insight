			MODE="CREATE" ENCODING="UTF-8" EOL="\n" HEADER="true" DELIMITER="-">
			<PARENT NAME="COPY_INPUT_1" RECORD_ID="REC2_2"
				START_AFTER_STATUS="1" />
			<COLUMN NAME="C1" SOURCE="C1" DATA_TYPE="STRING" LENGTH="10"
				FORMAT="" TRIM_TYPE="" />
		</FILE_WRITER>
	</COMPONENT>
-->    

</JOB>
