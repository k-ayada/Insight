<JOB>
	<GLOBALS>
		<TEMP_DIR><![CDATA[C:\temp\]]></TEMP_DIR>
		<LOG_INTERVEL>500000</LOG_INTERVEL>
	</GLOBALS>
	<COMPONENT ROOT_PKG="pub.ayada.insight.fixedFileReader">
		<FILE_READER NAME="FILE_READER_1" PATH="C:\TEMP\TEST_B.TXT"
			ENCODING="UTF-8" EOL="\r\n" BUFFER_RECS="50000" MAX_REC_LEN="15">
			<RECORDS WRITE_IGNORED_REC="true">
				<RECORD ID="REC1">
					<FILTER_EXP><![CDATA[@substr($rec,1,10)!="STRING0011"]]></FILTER_EXP>
					<COLUMN NAME="C1" START_POS="1" LENGTH="10" DATA_TYPE="STRING" 	FORMAT="" TRIM_TYPE="" />
					<COLUMN NAME="C2" START_POS="11" LENGTH="5" DATA_TYPE="INTEGER"	FORMAT="" TRIM_TYPE="" />
				</RECORD>
				<RECORD ID="REC2" MAX_REC_LEN="15">
					<FILTER_EXP><![CDATA[@substr($rec,1,10)!="STRING0011"]]></FILTER_EXP>
					<COLUMN NAME="C1" START_POS="1" LENGTH="15" DATA_TYPE="STRING" FORMAT="" TRIM_TYPE="" />
                    <COLUMN NAME="C2" SKIP_COLUMN="true" />
				</RECORD>
			</RECORDS>
		</FILE_READER>
	</COMPONENT>   
	<COMPONENT ROOT_PKG="pub.ayada.insight.fixedFileWriter" START_AFTER="2">
		<FILE_WRITER NAME="FILE_WRITER_1" PATH="C:\TEMP\TEST_OUT.TXT"
			MODE="CREATE" ENCODING="UTF-8" EOL="\n" HEADER="true" DELIMITER="\t">
			<PARENT NAME="FILE_READER_1" RECORD_ID="REC1"
				START_AFTER_STATUS="1" />
			<COLUMN NAME="C2" SOURCE="C2" DATA_TYPE="INTEGER" LENGTH="5"
				FORMAT="" TRIM_TYPE="" />
			<COLUMN NAME="C1" SOURCE="C1" DATA_TYPE="STRING" LENGTH="10"
				FORMAT="" TRIM_TYPE="" />
		</FILE_WRITER>
	</COMPONENT>   
	<COMPONENT ROOT_PKG="pub.ayada.Insight.replicator" START_AFTER="2">
		<REPLICATOR NAME="COPY_INPUT_1" PARENT="FILE_READER_1" RECORD_ID="REC2">
			<OUT_RECORD ID="REC2_1">
				<COLUMN NAME="C1" SOURCE="C1" DATA_TYPE="STRING" />
			</OUT_RECORD>
              
			<OUT_RECORD ID="REC2_2">
				<COLUMN NAME="C1" DATA_TYPE="STRING" />
			</OUT_RECORD>
       
		</REPLICATOR>
	</COMPONENT>
	<COMPONENT ROOT_PKG="pub.ayada.insight.doNothing" START_AFTER="1">
		<DO_NOTHING NAME="DO_NOTHING_1" PARENT="COPY_INPUT_1"
			RECORD_ID="REC2_1" />
	</COMPONENT>
	<COMPONENT ROOT_PKG="pub.ayada.insight.spooler" START_AFTER="2">
		<SPOOLER NAME="SPOOL_1" PARENT="COPY_INPUT_1" RECORD_ID="REC2_2" />
	</COMPONENT>
